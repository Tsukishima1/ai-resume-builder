generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserResume {
  resumeId   String @id @map("_id") @default(cuid())
  title     String 
  userEmail String
  userName  String
}